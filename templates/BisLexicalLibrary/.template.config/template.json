{
  "author": "Ryann (FlipperPlz)",
  "classifications": [ "Library", "BisUtils" ],
  "name": "BisUtils Lexical Addon Library",
  "description": "A project for creating a abstract lexer to be used by another lexer.",
  "identity": "BisUtils.Library.LexLib",
  "groupIdentity": "BisUtils.Library.LexLib",
  "shortName": "bislexlib",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MyProject.Lex",
  "symbols": {
    "languageName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "RvLex",
      "replaces": "LanguageName"
    },
    "name": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "{language}Lex"
    },
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net7.0"
        }
      ],
      "defaultValue": "net7.0"
    }
  }
}
